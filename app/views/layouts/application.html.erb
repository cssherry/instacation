  <%= stylesheet_link_tag    'application', media: 'all'%>
  <%= javascript_include_tag 'https://maps.googleapis.com/maps/api/js?libraries=places',
                             'application'%>

  <script>
    var cloud_name = "<%= ENV['cloud_name'] %>";
    var upload_preset = "<%= ENV['upload_preset'] %>";
    var api_key = "<%= ENV['cloudinary_api'] %>";
    $.cloudinary.config({ cloud_name: cloud_name, api_key: api_key })
  </script>

  <%= csrf_meta_tags %>
</head>
<body>
