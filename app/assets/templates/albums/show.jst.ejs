<h1>
  <%= album.escape('title') %>
  <% if (editable) { %>
    <a href='#' class="btn btn-primary open-photo-form" role="button">+ New Photo</a>
  <% } %>

  <a href="<%= '#users/' +  userId %>" class="btn btn-primary">
    Back to User Page
  </a>
</h1>

<div class='photos-and-create'>
  <div class='photo-form'>

  </div>

  <div class='photos' id='links'>

  </div>
</div>

<div id="blueimp-gallery" class="blueimp-gallery blueimp-gallery-controls">
    <div class="slides"></div>
    <h3 class="title"></h3>
    <div class="description google-map"></div>
    <a class="prev">‹</a>
    <a class="next">›</a>
    <a class="close">×</a>
    <a class="play-pause"></a>
    <ol class="indicator"></ol>
</div>
